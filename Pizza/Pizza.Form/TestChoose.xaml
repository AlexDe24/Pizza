<Window x:Class="Pizza.Form.TestChoose"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pizza.Form"
        mc:Ignorable="d"
        Title="Админ\Клиент" Height="200" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Name="Client" Template="{StaticResource ButtonOwal}" Content="Клиент" Height="25" Margin="5" Click="Client_Click"/>
            <Button Name="Operator" Template="{StaticResource ButtonOwal}" Content="Оператор" Height="25" Margin="5" Click="Operator_Click"/>
        </StackPanel>
    </Grid>
</Window>
